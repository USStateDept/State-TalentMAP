  "scripts": {
    "start": "node scripts/start.js",
    "start:dashboard": "webpack-dashboard node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node --expose-gc scripts/test.js --env=jsdom --no-cache --coverage --maxWorkers=3 --logHeapUsage",
    "test:local": "node --expose-gc scripts/test.js --env=jsdom --cache --coverage -u --logHeapUsage",
    "server": "yarn run build && node src/server.js",
    "analyze": "codeclimate analyze -f html > codeclimate.html",
    "analyze:eslint": "codeclimate analyze -e eslint:eslint-3 -f html > codeclimate.html",
    "analyze:scss": "codeclimate analyze -e scss-lint -f html > codeclimate.html",
    "stats": "NODE_ENV=production webpack --json --config ./config/webpack.config.prod.js > stats.json",
    "bundlesize": "bundlesize",
    "postinstall": "npm install -g cross-os && cross-os babel-deps"
  },
  "cross-os": {
    "babel-deps": {
      "linux": "test -n \"$NOYARNPOSTINSTALL\" || ./node_modules/@babel/cli/bin/babel.js --presets @babel/preset-env ./node_modules/@sweetalert --out-dir ./node_modules/@sweetalert",
      "darwin": "test -n \"$NOYARNPOSTINSTALL\" || ./node_modules/@babel/cli/bin/babel.js --presets @babel/preset-env ./node_modules/@sweetalert --out-dir ./node_modules/@sweetalert",
      "win32": "node ./node_modules/@babel/cli/bin/babel.js --presets @babel/preset-env ./node_modules/@sweetalert --out-dir ./node_modules/@sweetalert"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!src/Components/**/index.js",
      "!src/Components/StaticDevContent/*",
      "!src/actions/showStaticContent.js",
      "!src/reducers/showStaticContent.js",
      "!src/Components/CSV/**/*.{js,jsx}",
      "!src/polyfills/**/*.{js,jsx}",
      "!src/testUtilities/**/*"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock",
      "mock-local-storage"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^Components(.*)$": "<rootDir>/src/Components$1",
      "^actions(.*)$": "<rootDir>/src/actions$1",
      "^Constants(.*)$": "<rootDir>/src/Constants$1",
      "^Containers(.*)$": "<rootDir>/src/Containers$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^utilities": "<rootDir>/src/utilities.js",
      "^flags": "<rootDir>/src/flags.js",
      "^hooks": "<rootDir>/src/hooks.js",
      "^react-native$": "react-native-web"
    }
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "500 kB"
    }
  ]
}
